<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mobileassignment2.teeth_wars.Register"
    android:background="@drawable/cartoonbg">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Register Screen"
        android:id="@+id/textView"
        android:layout_x="100dp"
        android:layout_y="151dp"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="First Name:"
        android:id="@+id/textView3"
        android:layout_x="1dp"
        android:layout_y="290dp"
        android:textColor="#050505" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Last Name:"
        android:id="@+id/textView4"
        android:layout_x="1dp"
        android:layout_y="339dp"
        android:textColor="#040404" />

    <EditText
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText"
        android:background="@drawable/rect_text_edit"
        android:layout_x="102dp"
        android:layout_y="241dp"
        android:textColor="#000000" />

    <EditText
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:background="@drawable/rect_text_edit"
        android:layout_x="102dp"
        android:layout_y="290dp"
        android:textColor="#000000" />


    <ImageView
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:id="@+id/imageView"
        android:layout_x="95dp"
        android:layout_y="-4dp"
        android:background="@drawable/toothp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Step One, Input your Details Below"
        android:id="@+id/textView2"
        android:layout_x="36dp"
        android:layout_y="185dp"
        android:textColor="#000000" />

    <Button
        android:layout_width="123dp"
        android:layout_height="wrap_content"
        android:text="Step Two"
        android:id="@+id/button2"
        android:layout_x="118dp"
        android:layout_y="488dp"
        android:onClick="onSignUpClick" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Username:"
        android:id="@+id/textView5"
        android:layout_x="1dp"
        android:layout_y="241dp"
        android:textColor="#040404" />

    <EditText
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText3"
        android:background="@drawable/rect_text_edit"
        android:layout_x="102dp"
        android:layout_y="339dp"
        android:textColor="#000000" />

    <EditText
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editText6"
        android:background="@drawable/rect_text_edit"
        android:layout_x="102dp"
        android:layout_y="388dp"
        android:textColor="#000000" />

    <EditText
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editText7"
        android:background="@drawable/rect_text_edit"
        android:layout_x="102dp"
        android:layout_y="437dp"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Password:"
        android:id="@+id/textView9"
        android:layout_x="1dp"
        android:layout_y="388dp"
        android:textColor="#030303" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Password:"
        android:id="@+id/textView10"
        android:layout_x="1dp"
        android:layout_y="437dp"
        android:textColor="#050505" />

</AbsoluteLayout>
